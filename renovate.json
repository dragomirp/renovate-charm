{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base"
  ],
  "enabledManagers": ["poetry", "github-actions"],
  "packageRules": [
    {
      "matchManagers": ["poetry"],
      "groupName": "python"
    }, {
      "matchManagers": ["github-actions"],
      "groupName": "actions"
    }
  ],
  "ignoreDeps": []
}
